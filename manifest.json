{
    "author": "seven communications GmbH & Co. KG",
    "description": "Send SMS via seven",
    "gateway_specific_settings": {
        "webthings": {
            "exec": "{nodeLoader} {path}",
            "primary_type": "notifier",
            "strict_max_version": "*",
            "strict_min_version": "0.10.0"
        }
    },
    "homepage_url": "https://github.com/seven-io/webthings#readme",
    "id": "seven",
    "license": "MIT",
    "manifest_version": 1,
    "name": "seven",
    "options": {
        "default": {
            "apiKey": "",
            "from": "",
            "recipients": []
        },
        "schema": {
            "properties": {
                "apiKey": {
                    "title": "The API key of the seven user",
                    "type": "string"
                },
                "from": {
                    "title": "An optional sender identifier",
                    "type": "string"
                },
                "recipients": {
                    "items": {
                        "title": "A recipient",
                        "type": "string"
                    },
                    "title": "A list of recipients",
                    "type": "array"
                }
            },
            "required": [
                "apiKey",
                "recipients"
            ],
            "type": "object"
        }
    },
    "short_name": "seven",
    "version": "0.0.2"
}
